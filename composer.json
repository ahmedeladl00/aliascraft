{
    "name": "ahmedeladl00/aliascraft",
    "description": "A PHP library to register and execute custom command aliases with Laravel integration.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "AliasCraft\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AliasCraft\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Ahmed T. Eladl",
            "email": "ahmed.eladl@rub.de"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^11.43"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "AliasCraft\\AliasCraftServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
